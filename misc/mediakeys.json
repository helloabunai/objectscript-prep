{
    "title": "Custom Media Key Bindings",
    "description": "Map cmd+shift+Z to media-prev",
    "manipulators": [
        {
        "type": "basic",
        "from": {
            "key_code": "z",
            "modifiers": {
            "mandatory": ["left_command", "left_shift"]
            }
        },
        "to": [
            {
            "consumer_key_code": "rewind"
            }
        ]
        },
        {
            "type": "basic",
            "from": {
                "key_code": "x",
                "modifiers": {
                "mandatory": ["left_command", "left_shift"]
                }
            },
            "to": [
                {
                "consumer_key_code": "play_or_pause"
                }
            ]
        },
        {
            "type": "basic",
            "from": {
                "key_code": "c",
                "modifiers": {
                "mandatory": ["left_command", "left_shift"]
                }
            },
            "to": [
                {
                "consumer_key_code": "fast_forward"
                }
            ]
        }
    ]
}
  